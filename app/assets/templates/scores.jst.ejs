<h2 class='header'>High Scores</h2>
<table>
<% var index = 0 %>
<% highScores.each(function (highScore) { %>
  <% if (index < 10) { %>
    <% index++ %>
  <tr>
    <td><%= highScore.escape('name') %></td>
    <td><%= highScore.escape('score') %></td>
  </tr>
  <% } %>
<% }) %>
</table>
<ul>
  <li class='home'><span>H</span><span>o</span><span>m</span><span>e</span></li>
  <li class='game'><span>P</span><span>l</span><span>a</span><span>y</span></li>
  <li class='controls'><span>C</span><span>o</span><span>n</span><span>t</span><span>r</span><span>o</span><span>l</span><span>s</span></li>
</ul>
